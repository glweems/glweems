{
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [
          [
            "src",
            "./src"
          ],
          [
            "styled",
            "./src/styled"
          ],
          [
            "theme",
            "./src/styled/theme.js"
          ],
          [
            "src/data",
            "./src/data.js"
          ],
          [
            "src/state",
            "./src/state"
          ],
          [
            "@",
            "./src/components"
          ],
          [
            "elements",
            "./src/styled/elements.js"
          ],
          [
            "theme",
            "./src/styled/theme.js"
          ],
          [
            "state",
            "./src/state"
          ]
        ],
        "extensions": [
          ".ts",
          ".js",
          ".jsx",
          ".json"
        ]
      }
    }
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "impliedStrict": true,
      "classes": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "prettier"
  ]
}
