{
  "name": "gatsby-source-behance",
  "version": "1.0.0",
  "description": "Gatsby source plugin for loading information from Behance",
  "main": "gatsby-node.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore __tests__"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/glweems/gatsby-source-behance"
  },
  "bugs": {
    "url": "https://github.com/glweems/gatsby-source-behance/issues"
  },
  "homepage": "https://github.com/glweems/gatsby-source-behance",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "behance"
  ],
  "author": "Garrett Weems <gwgraphicdesign@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.4",
    "crypto": "^1.0.1"
  },
  "peerDependencies": {
    "gatsby": ">=2"
  }
}
