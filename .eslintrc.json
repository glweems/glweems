{
  "parser": "babel-eslint",
  "extends": [
    "wesbos"
  ],
  "rules": {
    "import/no-unresolved": false,
    "no-nested-ternary": 0,
    "camelcase": 0,
    "undefined": 0,
    "react/display-name": 0,
    "no-unused-expressions": 0,
    "react/jsx-no-undef": 0,
    "no-undef": 0,
    "react/no-danger": 0
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  }
}
