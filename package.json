{
  "name": "glweems",
  "description": "Personal Portfolio Website",
  "version": "1.0.1",
  "author": "Garrett Weems <gwgraphicdesign@gmail.com>",
  "keywords": [
    "gatsby",
    "react",
    "developer",
    "portfolio"
  ],
  "license": "MIT",
  "scripts": {
    "start": "run-p start:**",
    "start:app": "npm run develop",
    "start:lambda": "netlify-lambda serve src/lambda --config ./webpack.functions.js",
    "build": "run-p build:**",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build src/lambda --config ./webpack.functions.js",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "lint": "eslint -c .eslintrc --ignore-path .gitignore ./src",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:fix": "eslint -c .eslintrc --ignore-path .gitignore ./src --fix",
    "now-build": "npm run build",
    "postinstall": "node node_modules/husky/lib/installer/bin install",
    "test": "echo \"no test specified\" && exit 0"
  },
  "repository": "github:glweems/glweems",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix",
      "pre-push": "npm run build"
    }
  },
