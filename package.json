    "clean": "gatsby clean",
    "build": "gatsby build",
    "start": "gatsby develop -H 0.0.0.0",
    "dev": "concurrently \"npm run start\" \"npm run gen:watch\"",
    "start:host": "gatsby develop -H 0.0.0.0",
    "restart": "gatsby clean && npm run start",
    "format": "prettier --ignore-path .gitignore --write \"src/**/*.{ts,tsx,json,yml,md}\"",
    "lint:es": "eslint --ignore-path .gitignore \"src/**/*.{ts,tsx}\"",
    "lint:style": "stylelint --ignore-path .gitignore \"src/**/*.{ts,tsx}\"",
    "lint": "npm run lint:es && npm run lint:style",
    "lint:fix": "npm run format && npm run lint:es --fix && npm run lint:style",
    "type-check": "tsc",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "gatsby build"
    }
