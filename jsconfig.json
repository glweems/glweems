{
  "compilerOptions": {
    "target": "es2017",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/components/*"
      ],
      "~": [
        "src/styled/"
      ],
      "elements": [
        "src/styled/elements"
      ],
      "src/*": [
        "src/*"
      ],
      "pages/*": [
        "src/pages/*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "public"
  ],
  "module": "commonjs"
}
