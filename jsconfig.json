{
  "compilerOptions": {
    "target": "es2017",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/components/*"
      ],
      "~": [
        "./node_modules/*"
      ],
      "my-components": [
        "src/components/index.js"
      ],
      "src/*": [
        "src/*"
      ],
      "pages/*": [
        "src/pages/w*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "module": "commonjs"
}
