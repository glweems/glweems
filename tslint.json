{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/**.ts",
      "coverage/lcov-report/*.js",
      "public/**"
    ]
  },
  "rules": {
    "no-implicit-dependencies": false,
    "allowSyntheticDefaultImports": true,
    "interface-name": false,
    "jsx-no-lambda": false,
    "semicolon": false
  }
}
