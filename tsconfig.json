{
  "compilerOptions": {
    "strict": true,
    "types": ["node"],
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "plugins": []
  },
  "include": ["src/**/*.tsx", "src/**/*.ts", "src/**/*.svg", "createPages.ts"],
  "exclude": ["node_modules/"]
}
