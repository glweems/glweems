{
  "compilerOptions": {
    "types": ["node"],
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "pretty": true,
    "noEmit": true
  },
  "exclude": ["node_modules"]
}
