{
  "version": 2,
  "name": "glweems",
  "public": true,
  "alias": [
    "glweems.now.sh"
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "env": {
    "SENTRY_TOKEN": "@sentry_token",
    "BEHANCE_TOKEN": "@behance_token",
    "GITHUB_TOKEN": "@github_token"
  }
}
